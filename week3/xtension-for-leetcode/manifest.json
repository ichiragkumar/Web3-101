{
    "manifest_version": 3,
    "name": "Twitter Like Tracker",
    "version": "1.0",
    "description": "Stores liked tweets in local storage.",
    "permissions": ["storage"],
    "host_permissions": ["https://*.x.com/*"],
    "content_scripts": [
      {
        "matches": ["https://*.x.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup/popup.html"
    }
  }
  